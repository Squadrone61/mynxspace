{
  "name": "mynxspace",
  "version": "0.0.0",
  "description": "Experimenting environment for Angular",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "preinstall": "if exist dist (rmdir /S /Q dist)",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "npm install && npm run front:prod && npm run api:prod",
    "front:dev": "npm run dev && nx run frontendapp:serve",
    "front:prod": "npm run build:prod && nx run frontendapp:build:production",
    "api:dev": "nx run api:serve",
    "api:prod": "nx run api:build:production && node dist/apps/api/main.js",
    "update": "nx migrate latest",
    "dev": "SET TAILWIND_MODE=watch SET NODE_ENV=development postcss  apps/frontendapp/src/styles.pcss -o apps/frontendapp/src/styles.css -w",
    "build:prod": "SET TAILWIND_MODE=build SET NODE_ENV=production postcss  apps/frontendapp/src/styles.pcss -o apps/frontendapp/src/styles.css",
    "generate-icons": "svg-generator"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.9",
    "@angular/cdk": "^11.2.8",
    "@angular/common": "^11.2.9",
    "@angular/compiler": "^11.2.9",
    "@angular/core": "^11.2.9",
    "@angular/forms": "^11.2.9",
    "@angular/platform-browser": "^11.2.9",
    "@angular/platform-browser-dynamic": "^11.2.9",
    "@angular/router": "^11.2.9",
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/serve-static": "^2.1.4",
    "@ngneat/svg-icon": "^3.1.1",
    "@nrwl/angular": "12.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.6.7",
    "tailwindcss": "^2.1.1",
    "tslib": "^2.2.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.8",
    "@angular-eslint/eslint-plugin": "~2.0.2",
    "@angular-eslint/eslint-plugin-template": "~2.0.2",
    "@angular-eslint/template-parser": "~2.0.2",
    "@angular/cli": "~11.2.8",
    "@angular/compiler-cli": "^11.2.9",
    "@angular/language-service": "^11.2.9",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.15",
    "@ngneat/svg-generator": "^2.0.0",
    "@nrwl/cli": "12.0.0",
    "@nrwl/cypress": "12.0.0",
    "@nrwl/eslint-plugin-nx": "12.0.0",
    "@nrwl/jest": "12.0.0",
    "@nrwl/linter": "12.0.0",
    "@nrwl/nest": "^12.0.0",
    "@nrwl/node": "12.0.0",
    "@nrwl/tao": "12.0.0",
    "@nrwl/workspace": "12.0.0",
    "@types/jest": "26.0.22",
    "@types/node": "14.14.37",
    "@typescript-eslint/eslint-plugin": "4.21.0",
    "@typescript-eslint/parser": "4.21.0",
    "cypress": "^6.0.1",
    "dotenv": "8.2.0",
    "eslint": "7.24.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.11.2",
    "jest": "26.6.3",
    "jest-preset-angular": "8.4.0",
    "postcss": "^8.2.9",
    "postcss-cli": "^8.3.1",
    "prettier": "2.2.1",
    "run-script-webpack-plugin": "^0.0.11",
    "ts-jest": "26.4.0",
    "ts-loader": "^5.4.5",
    "ts-node": "~9.1.1",
    "typescript": "~4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "^1.7.2"
  },
  "svgGenerator": {
    "outputPath": "./libs/icon/src/lib/svg",
    "prefix": "",
    "srcPath": "./libs/icon/src/lib/assets/svg",
    "svgoConfig": {
      "plugins": [
        {
          "removeDimensions": true,
          "cleanupAttrs": true
        }
      ]
    }
  }
}
