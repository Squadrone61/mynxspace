{
  "name": "mynxspace",
  "version": "0.0.0",
  "description": "Experimenting environment for Angular",
  "license": "MIT",
  "scripts": {
    "preinstall": "if exist dist (rmdir /S /Q dist)",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "npm install && npm run front:prod && npm run api:prod",
    "front:dev": "nx run frontendapp:serve",
    "front:prod": "npm run css:prod && nx run frontendapp:build:production",
    "api:dev": "nx run api:serve",
    "api:prod": "nx run api:build:production && node dist/apps/api/main.js",
    "update": "nx migrate latest",
    "css:dev": "SET TAILWIND_MODE=watch && SET NODE_ENV=development && postcss ./apps/frontendapp/src/styles.pcss -o ./apps/frontendapp/src/styles.css -w",
    "css:prod": "SET TAILWIND_MODE=build && SET NODE_ENV=production && postcss  apps/frontendapp/src/styles.pcss -o apps/frontendapp/src/styles.css",
    "generate-icons": "svg-generator"
  },
  "private": true,
  "dependencies": {
    "@angular-three/controls": "1.0.0-beta.28",
    "@angular-three/core": "1.0.0-beta.28",
    "@angular/animations": "12.0.0",
    "@angular/cdk": "12.0.0",
    "@angular/common": "12.0.0",
    "@angular/compiler": "12.0.0",
    "@angular/core": "12.0.0",
    "@angular/forms": "12.0.0",
    "@angular/platform-browser": "12.0.0",
    "@angular/platform-browser-dynamic": "12.0.0",
    "@angular/router": "12.0.0",
    "@nestjs/common": "^7.6.15",
    "@nestjs/core": "^7.6.15",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/serve-static": "^2.1.4",
    "@ngneat/svg-icon": "^3.1.0",
    "@ngrx/component-store": "12.0.0",
    "@nrwl/angular": "12.3.5",
    "@tweenjs/tween.js": "^18.6.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.6.7",
    "three": "^0.128.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.0.0",
    "@angular-eslint/eslint-plugin": "12.0.0",
    "@angular-eslint/eslint-plugin-template": "12.0.0",
    "@angular-eslint/template-parser": "12.0.0",
    "@angular/cli": "12.0.0",
    "@angular/compiler-cli": "12.0.0",
    "@angular/language-service": "12.0.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^7.6.15",
    "@ngneat/svg-generator": "^2.0.0",
    "@nrwl/cli": "12.3.5",
    "@nrwl/cypress": "12.3.5",
    "@nrwl/eslint-plugin-nx": "12.3.5",
    "@nrwl/jest": "12.3.5",
    "@nrwl/linter": "12.3.5",
    "@nrwl/nest": "12.3.5",
    "@nrwl/node": "12.3.5",
    "@nrwl/tao": "12.3.5",
    "@nrwl/workspace": "12.3.5",
    "@types/jest": "26.0.22",
    "@types/node": "14.14.37",
    "@types/three": "0.127.1",
    "@typescript-eslint/eslint-plugin": "4.21.0",
    "@typescript-eslint/parser": "4.21.0",
    "autoprefixer": "^10.2.5",
    "cypress": "^6.0.1",
    "dotenv": "8.2.0",
    "eslint": "7.24.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.11.2",
    "jest": "26.6.3",
    "jest-preset-angular": "8.4.0",
    "postcss": "^8.2.13",
    "postcss-cli": "^8.3.1",
    "prettier": "2.2.1",
    "run-script-webpack-plugin": "^0.0.11",
    "tailwindcss": "^2.1.2",
    "ts-jest": "26.5.5",
    "ts-loader": "^5.4.5",
    "ts-node": "~9.1.1",
    "typescript": "4.2.4",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "^1.7.2"
  },
  "svgGenerator": {
    "outputPath": "./libs/icon/src/lib/svg",
    "prefix": "",
    "srcPath": "./libs/icon/src/lib/assets/svg",
    "svgoConfig": {
      "plugins": [
        {
          "removeDimensions": true,
          "cleanupAttrs": true
        }
      ]
    }
  }
}
